<!-- Scaffolding generated by Casein v5.1.1.5 -->

<h2>View page content</h2>

<%= form_for @page_content, :url => casein_page_content_path(@page_content), :html => { multipart: true } do |f| %>

	<%= render :partial => 'casein/page_contents/form', :locals => { :f => f } %>

	<p class="submits">
	  	<%= link_to 'Cancel', casein_page_contents_path, :class => 'btn btn-mini btn-danger' %>
		<%= f.submit "Save changes", :class => "btn btn-mini btn-primary" %>
	</p>

<% end %>

<%= content_for :sidebar do %>
	<li><%= link_to "#{casein_show_icon "th-list"}Back to list".html_safe, casein_page_contents_path %></li>
<% end %>